{
  "name": "kubernetes-gated-deployments",
  "version": "1.0.0",
  "description": "Gate Kubernetes deployments with AB tests",
  "main": "bin/daemon.js",
  "scripts": {
    "coverage": "nyc ./node_modules/mocha/bin/_mocha --recursive lib",
    "lint": "standard",
    "start": "./bin/daemon.js",
    "nodemon": "nodemon ./bin/daemon.js",
    "test": "mocha --recursive lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/kubernetes-gated-deployments"
  },
  "keywords": [
    "kubernetes",
    "aws"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "async-lock": "^1.2.0",
    "clone": "^2.1.2",
    "json-stream": "^1.0.0",
    "kubernetes-client": "^6.5.0",
    "make-promises-safe": "^4.0.0",
    "mann-whitney-utest": "^1.0.5",
    "moment": "^2.24.0",
    "object-hash": "^1.3.1",
    "pino": "^5.9.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "dotenv": "^6.1.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.0",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "standard": "^12.0.1"
  },
  "nyc": {
    "check-coverage": true,
    "reporter": [
      "cobertura",
      "json-summary",
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "config/",
      "coverage/",
      "bin/",
      "**/*.test.js"
    ],
    "lines": 4,
    "functions": 4,
    "all": true,
    "cache": false,
    "temp-directory": "./coverage/.nyc_output"
  }
}
